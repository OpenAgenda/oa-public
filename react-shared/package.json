{
  "name": "@openagenda/react-shared",
  "version": "2.4.1",
  "main": "lib/index.js",
  "jsnext:main": "esm/index.js",
  "module": "esm/index.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/OpenAgenda/oa-public.git"
  },
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "esm",
    "css",
    "scss"
  ],
  "scripts": {
    "start": "storybook dev -p 9001 --ci",
    "sb": "yarn start",
    "prepack": "yarn clean && yarn oa-intl compile && yarn build",
    "build": "yarn build:esm && yarn build:cjs && yarn build:scss",
    "build:esm": "babel --copy-files src -d esm -s --env-name esm",
    "build:cjs": "babel --copy-files src -d lib -s",
    "build:scss": "sass scss:css",
    "extract-messages": "yarn oa-intl",
    "clean": "rimraf esm lib css",
    "lint": "eslint -c .eslintrc '**/*.js'",
    "prettier": "prettier-eslint --write '**/*.js'",
    "lint-staged": "lint-staged",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.23.2",
    "@emotion/react": "^11.11.1",
    "@openagenda/intl": "^1.1.3",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "bytes": "^3.1.0",
    "classnames": "^2.3.1",
    "core-js": "^3.29.1",
    "history": "^4.10.1",
    "hoist-non-react-statics": "^3.3.1",
    "lodash": "^4.17.20",
    "marked": "^2.1.3",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
    "react-bootstrap": "^0.33.1",
    "react-cookie-consent": "^7.2.1",
    "react-date-range": "^1.4.0",
    "react-dropzone": "^11.2.0",
    "react-intl": "^5.12.2",
    "react-markdown": "^6.0.0",
    "react-redux": "^7.1.3",
    "react-router-config": "^5.1.1",
    "react-use": "^17.3.2",
    "react-waypoint": "^9.0.2",
    "redial": "^0.5.0",
    "sanitize-html": "^1.20.1",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@cfaester/enzyme-adapter-react-18": "^0.6.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@loadable/component": "^5.14.1",
    "@openagenda/babel-preset": "^1.5.0",
    "@openagenda/bs-templates": "^2.5.0",
    "@openagenda/eslint-config": "^1.4.0",
    "@storybook/addon-actions": "^7.4.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react-webpack5": "^7.4.2",
    "babel-jest": "^29.7.0",
    "dedent": "^0.7.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.24.0",
    "final-form": "^4.20.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.2.11",
    "msw": "^0.49.2",
    "msw-storybook-addon": "^1.10.0",
    "prettier-eslint-cli": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "redux": "^4.0.5",
    "rimraf": "^3.0.1",
    "sass": "^1.69.5",
    "storybook": "^7.6.4"
  },
  "peerDependencies": {
    "@loadable/component": "^5.12.0",
    "react": "^18.2.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^3.0.0",
    "redux": "^4.0.5"
  },
  "peerDependenciesMeta": {
    "@loadable/component": {
      "optional": true
    },
    "react-router": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    },
    "react-select": {
      "optional": true
    },
    "redux": {
      "optional": true
    }
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
