{
  "name": "@openagenda/intl",
  "version": "1.1.3",
  "author": "KÃ©vin Berthommier <kevin.bertho@gmail.com>",
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/OpenAgenda/oa-public.git",
    "directory": "intl"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "esm",
    "scripts"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./scripts/*": "./scripts/*.js",
    "./*": {
      "import": "./esm/*.js",
      "require": "./lib/*.js",
      "default": "./lib/*.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "oa-intl": "./scripts/cli.js"
  },
  "scripts": {
    "prepack": "yarn clean && yarn build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "babel --copy-files src -d esm -s --env-name esm",
    "build:cjs": "babel --copy-files src -d lib -s",
    "clean": "rimraf esm lib",
    "lint": "eslint -c .eslintrc '**/*.js'",
    "prettier": "prettier-eslint --write '**/*.js'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.23.2",
    "@formatjs/cli": "^4.8.3",
    "@formatjs/intl": "^1.6.7",
    "core-js": "^3.29.1",
    "dedent": "^0.7.0",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "tmp": "^0.2.1",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@openagenda/babel-preset": "^1.5.0",
    "@openagenda/eslint-config": "^1.4.0",
    "eslint": "^8.24.0",
    "lint-staged": "^10.2.11",
    "prettier-eslint-cli": "^7.1.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
