<div class="side-content cbpgmp small-map">
  <div class="map-canvas leaflet-container leaflet-fade-anim" tabindex="0" style="position: relative;">

    <div class="leaflet-map-pane" style="transform: translate3d(0px, 0px, 0px);">
      <div class="leaflet-tile-pane">
        <div class="leaflet-layer">
          <div class="leaflet-tile-container"></div>
          <div class="leaflet-tile-container leaflet-zoom-animated">
            <img class="leaflet-tile leaflet-tile-loaded" src="//c.tiles.mapbox.com/v3/foursquare.meku766r/17/66333/45116.png" style="height: 256px; width: 256px; left: -171px; top: 8px;">
            <img class="leaflet-tile leaflet-tile-loaded" src="//a.tiles.mapbox.com/v3/foursquare.meku766r/17/66334/45116.png" style="height: 256px; width: 256px; left: 85px; top: 8px;">
            <img class="leaflet-tile leaflet-tile-loaded" src="//b.tiles.mapbox.com/v3/foursquare.meku766r/17/66333/45115.png" style="height: 256px; width: 256px; left: -171px; top: -248px;">
            <img class="leaflet-tile leaflet-tile-loaded" src="//c.tiles.mapbox.com/v3/foursquare.meku766r/17/66334/45115.png" style="height: 256px; width: 256px; left: 85px; top: -248px;">
            <img class="leaflet-tile leaflet-tile-loaded" src="//a.tiles.mapbox.com/v3/foursquare.meku766r/17/66333/45117.png" style="height: 256px; width: 256px; left: -171px; top: 264px;">
            <img class="leaflet-tile leaflet-tile-loaded" src="//b.tiles.mapbox.com/v3/foursquare.meku766r/17/66334/45117.png" style="height: 256px; width: 256px; left: 85px; top: 264px;">
          </div>
        </div>
      </div>
      <div class="leaflet-objects-pane">
        <div class="leaflet-shadow-pane"></div>
        <div class="leaflet-overlay-pane">
          <svg class="leaflet-zoom-animated" width="333" height="313" viewBox="-7 -6 333 313" style="transform: translate3d(-7px, -6px, 0px);"></svg>
        </div>
        <div class="leaflet-marker-pane">
          <img src="//s3-eu-west-1.amazonaws.com/cibulstatic/markerIcon.png" class="leaflet-marker-icon leaflet-zoom-animated leaflet-clickable" tabindex="0" style="
            margin-left: -10px; 
            margin-top: 60px; 
            width: 18px; 
            height: 25px; 
            transform: translate3d(160px, 150px, 0px); 
            z-index: 1150;
          ">
        </div>
        <div class="leaflet-popup-pane">
          <div class="leaflet-popup leaflet-zoom-animated" style="
            opacity: 1; 
            transform: translate3d(160px, 150px, 0px); 
            bottom: -75px; 
            left: -86px;">
            <a class="leaflet-popup-close-button" href="#close">×</a>
            <div class="leaflet-popup-content-wrapper">
              <div class="leaflet-popup-content" style="width: 170px;">
                <div class="map-location">

                  <span class="count">
                    <span>1</span>
                    <label> event</label>
                  </span>

                  <div class="oa-location-image">
                    <img src="//cibul.s3.amazonaws.com/location74259210.jpg">
                  </div>

                  <span class="oa-location-details">
                    <p>Atrium de Chaville</p>
                    <span class="map-address">3, parvis Robert Schuman 92370 Chaville</span>
                    <span class="map-postalcode">92370</span>
                    <span class="map-city">Chaville</span>
                    <span class="map-department">Hauts-de-Seine</span>
                    <span class="map-region">Île-de-France</span>
                  </span>

                </div>
              </div>
            </div>
            <div class="leaflet-popup-tip-container">
              <div class="leaflet-popup-tip"></div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="leaflet-control-container">
      <div class="leaflet-top leaflet-left">
        <div class="leaflet-control-zoom leaflet-bar leaflet-control">
          <a class="leaflet-control-zoom-in" href="#" title="Zoom in">+</a>
          <a class="leaflet-control-zoom-out" href="#" title="Zoom out">-</a>
        </div>
      </div>
      <div class="leaflet-top leaflet-right"></div>
      <div class="leaflet-bottom leaflet-left"></div>
      <div class="leaflet-bottom leaflet-right">
        <div class="leaflet-control-attribution leaflet-control">
          <a href="http://leafletjs.com" title="A JS library for interactive maps">Leaflet</a> | Map data © OpenStreetMap contributors
        </div>
      </div>
    </div>

  </div>

  <div class="map-sync js_map_sync">
    <input type="checkbox" id="map-sync" class="js_sync_checkbox">
    <label for="map-sync">Search when I move the map</label>
  </div>

</div>