{
  "name": "@openagenda/sdk-js",
  "version": "2.0.1",
  "repository": "git+ssh://git@github.com/OpenAgenda/oa-public.git",
  "author": "Bertho <kevin.bertho@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    },
    "./api": {
      "import": "./esm/api/index.js",
      "require": "./lib/api/index.js"
    },
    "./schema-org": {
      "import": "./esm/schema-org/index.js",
      "require": "./lib/schema-org/index.js"
    }
  },
  "scripts": {
    "prepack": "yarn clean && yarn build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "babel --copy-files src -d esm -s --env-name esm",
    "build:cjs": "babel --copy-files src -d lib -s",
    "clean": "rimraf esm lib",
    "test": "yarn test:unit",
    "test:unit": "jest --config=jest.config.unit.js",
    "test:e2e": "jest --config=jest.config.e2e.js",
    "lint": "eslint -c .eslintrc 'src/**/*.js'",
    "prettier": "prettier-eslint --write 'src/**/*.js'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.17",
    "@openagenda/intl": "workspace:^",
    "axios": "^0.26.1",
    "core-js": "^3.10.2",
    "lodash": "^4.17.15",
    "qs": "^6.10.3",
    "regenerator-runtime": "^0.13.7",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@openagenda/axios-mock-adapter": "^1.21.2",
    "@openagenda/babel-preset": "^1.5.0",
    "@openagenda/eslint-config": "^1.4.0",
    "babel-jest": "^26.6.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "eslint": "^8.24.0",
    "jest": "^28.1.2",
    "lint-staged": "^10.2.11",
    "moment": "^2.27.0",
    "prettier-eslint-cli": "^7.1.0",
    "rimraf": "^3.0.1",
    "sinon": "^4.4.5"
  }
}
