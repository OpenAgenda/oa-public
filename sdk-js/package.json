{
  "name": "@openagenda/sdk-js",
  "version": "1.3.1",
  "main": "dist/index.js",
  "repository": "git+ssh://git@github.com/OpenAgenda/oa-public.git",
  "author": "Bertho <kevin.bertho@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d dist -s",
    "test": "yarn test:unit",
    "test:unit": "jest --config=jest.config.unit.js",
    "test:e2e": "jest --config=jest.config.e2e.js",
    "prepack": "yarn build",
    "lint": "eslint -c .eslintrc '**/*.js'",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc $(pwd)'/**/*.js'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.17",
    "axios": "^0.26.1",
    "core-js": "^3.10.2",
    "lodash": "^4.17.15",
    "qs": "^6.10.3",
    "regenerator-runtime": "^0.13.7",
    "superagent": "^3.8.2",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@openagenda/axios-mock-adapter": "^1.21.2",
    "@openagenda/babel-preset": "^1.4.1",
    "@openagenda/eslint-config": "^1.3.1",
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "moment": "^2.27.0",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.1",
    "sinon": "^4.4.5"
  }
}
