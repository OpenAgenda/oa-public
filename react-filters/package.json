{
  "name": "@openagenda/react-filters",
  "version": "2.13.3",
  "type": "module",
  "main": "build/index.js",
  "sideEffects": false,
  "license": "MIT",
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build",
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./locales": {
      "import": "./build/locales/index.js",
      "require": "./build/locales/index.cjs"
    },
    "./*": {
      "import": "./build/*.js",
      "require": "./build/*.cjs"
    }
  },
  "scripts": {
    "sb": "yarn storybook",
    "prepack": "yarn clean && oa-intl compile --esm && yarn build & yarn dist",
    "build": "tsup",
    "dist": "webpack-cli --config webpack.config.cjs",
    "extract-messages": "oa-intl --esm",
    "clean": "rimraf esm lib",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "start": "concurrently -r -k -i -n dist,sb \"yarn dist -w\" \"yarn storybook\"",
    "storybook": "NODE_OPTIONS=\"--max-old-space-size=5120\" storybook dev",
    "example": "cd example && nodemon server.js"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.28.4",
    "@emotion/react": "^11.11.1",
    "@formatjs/cli": "^2.11.3",
    "@openagenda/intl": "^1.1.6",
    "@openagenda/leaflet-gesture-handling": "^1.5.0",
    "@openagenda/react-date-range": "^2.0.2",
    "@openagenda/react-portal-ssr": "^1.0.3",
    "@openagenda/react-shared": "^2.4.6",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.29.1",
    "date-fns": "^2.11.1",
    "date-fns-tz": "^2.0.1",
    "final-form": "^4.20.2",
    "fuse.js": "^7.1.0",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.20",
    "qs": "^6.9.1",
    "react-final-form": "^6.5.3",
    "react-final-form-listeners": "^1.0.3",
    "react-intl": "^6.8.4",
    "react-leaflet": "^4.2.0",
    "react-query": "^3.31.0",
    "react-select": "^5.8.3",
    "react-uid": "^2.3.0",
    "react-use": "^17.6.0",
    "slate-dom": "^0.112.2",
    "swiper": "^11.2.6",
    "use-debounce": "^10.0.4",
    "use-local-storage-state": "^13.0.0"
  },
  "devDependencies": {
    "@openagenda/axios-mock-adapter": "^1.21.0",
    "@openagenda/eslint-config": "^1.4.0",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react-webpack5": "^9.0.8",
    "babel-loader": "^9.1.3",
    "body-parser": "^1.19.0",
    "bootstrap": "4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.9",
    "express": "^4.17.1",
    "jest": "^29.7.0",
    "mkdirp": "^3.0.1",
    "nodemon": "^2.0.15",
    "raw-loader": "^4.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^3.0.1",
    "sass": "^1.83.1",
    "source-map-loader": "^2.0.1",
    "storybook": "^9.0.8",
    "terser-webpack-plugin": "^5.3.9",
    "tsup": "^8.3.5",
    "webpack": "^5.53.0",
    "webpack-cli": "^5.1.4",
    "webpackbar": "^5.0.0-3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
