{
  "name": "@openagenda/react-filters",
  "version": "1.4.0",
  "main": "lib/index.js",
  "jsnext:main": "esm/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "prepack": "yarn clean && yarn build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "babel --copy-files src -d esm -s --env-name esm",
    "build:cjs": "babel --copy-files src -d lib -s",
    "extract-messages": "yarn oa:extract-messages",
    "clean": "rimraf esm lib",
    "lint": "eslint -c .eslintrc '**/*.js'",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc $(pwd)'/**/*.js'",
    "lint-staged": "lint-staged",
    "start": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.17",
    "@emotion/react": "^11.1.5",
    "@formatjs/cli": "^2.11.3",
    "@openagenda/react-shared": "^2.2.0",
    "@raruto/leaflet-gesture-handling": "^1.3.4",
    "@stefanoruth/react-portal-ssr": "^2.0.3",
    "classnames": "^2.3.1",
    "core-js": "^3.10.2",
    "date-fns": "^2.11.1",
    "fuse.js": "^6.4.6",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
    "react-date-range": "^1.1.3",
    "react-final-form": "^6.5.3",
    "react-intl": "^5.12.2",
    "react-leaflet": "^3.1.0",
    "react-uid": "^2.3.0",
    "react-use": "^15.3.4",
    "use-debounce": "^6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@openagenda/babel-preset": "^1.4.1",
    "@openagenda/eslint-config": "^1.3.1",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "babel-loader": "^8.2.2",
    "eslint": "^6.8.0",
    "final-form": "^4.20.2",
    "lint-staged": "^10.2.11",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.1"
  },
  "peerDependencies": {
    "final-form": "^4.18.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
