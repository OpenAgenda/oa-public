{
  "name": "@openagenda/react-timingspicker",
  "version": "1.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/OpenAgenda/oa-public.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "main": "dist/index.js",
  "scripts": {
    "start": "storybook dev -p 9001",
    "test": "TZ=Europe/Paris jest --forceExit --detectOpenHandles",
    "build": "babel --copy-files src -d dist -s",
    "extract-messages": "yarn oa-intl",
    "clean": "rimraf dist",
    "prepack": "yarn clean && yarn oa-intl compile && yarn build",
    "lint": "eslint -c .eslintrc '**/*.js'",
    "prettier": "prettier-eslint --write '**/*.js'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.23.2",
    "@openagenda/intl": "^1.1.3",
    "@openagenda/react-shared": "^2.4.0",
    "classnames": "^2.3.1",
    "core-js": "^3.29.1",
    "date-fns": "^2.1.0",
    "final-form": "^4.20.2",
    "react-day-picker": "^7.3.0",
    "react-final-form": "^6.5.3",
    "react-icons": "^3.8.0",
    "react-intl": "^5.12.2",
    "react-modal": "^3.11.1",
    "react-resize-detector": "^4.2.1",
    "react-select": "^3.0.8",
    "react-text-mask": "^5.4.3",
    "rrule": "^2.6.8",
    "text-mask-addons": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@openagenda/babel-preset": "^1.5.0",
    "@openagenda/eslint-config": "^1.4.0",
    "@storybook/addon-viewport": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "babel-jest": "^29.7.0",
    "bootstrap": "3.3.5",
    "eslint": "^8.24.0",
    "jest": "^29.7.0",
    "lint-staged": "^10.2.11",
    "prettier-eslint-cli": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.1",
    "storybook": "^7.4.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
