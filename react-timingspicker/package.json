{
  "name": "@openagenda/react-timingspicker",
  "version": "1.2.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/OpenAgenda/oa-public.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "main": "dist/index.js",
  "scripts": {
    "start": "start-storybook -p 9001",
    "test": "TZ=Europe/Paris jest --forceExit --detectOpenHandles",
    "build": "babel --copy-files src -d dist -s",
    "extract-messages": "yarn oa:extract-messages",
    "clean": "rimraf dist",
    "prepack": "yarn clean && yarn build",
    "lint": "eslint -c .eslintrc '**/*.js'",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc $(pwd)'/**/*.js'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.16",
    "@openagenda/react-shared": "^2.1.1",
    "classnames": "^2.3.1",
    "core-js": "^3.10.2",
    "date-fns": "^2.1.0",
    "react-day-picker": "^7.3.0",
    "react-final-form": "^6.4.0",
    "react-icons": "^3.8.0",
    "react-intl": "^5.12.2",
    "react-modal": "^3.11.1",
    "react-resize-detector": "^4.2.1",
    "react-select": "^3.0.8",
    "react-text-mask": "^5.4.3",
    "rrule": "^2.6.3",
    "text-mask-addons": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@openagenda/babel-preset": "^1.4.1",
    "@openagenda/eslint-config": "^1.3.1",
    "@storybook/addon-viewport": "^6.1.0",
    "@storybook/addons": "^6.1.0",
    "@storybook/react": "^6.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^6.8.0",
    "final-form": "^4.20.1",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.1"
  },
  "peerDependencies": {
    "final-form": "^4.18.5",
    "react": "^16.12.0"
  }
}
