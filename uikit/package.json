{
  "name": "@openagenda/uikit",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/OpenAgenda/oa-public.git"
  },
  "homepage": "https://github.com/OpenAgenda/oa-public",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "chakra-cli tokens src/index.ts",
    "start": "start-storybook -c .storybook -p 9001 --ci",
    "prepack": "tsup src --clean --format esm,cjs --dts",
    "lint": "eslint -c .eslintrc '**/*.@(js|jsx|ts|tsx|mjs)'",
    "prettier": "prettier-eslint --write '**/*.@(js|jsx|ts|tsx|mjs)'"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.0.7",
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.5",
    "@chakra-ui/theme-tools": "^2.0.12",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "framer-motion": "^7.5.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.1.8",
    "@openagenda/eslint-config": "^1.4.0",
    "@openagenda/tsconfig": "^0.0.0",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/react": "^6.5.9",
    "eslint": "^8.24.0",
    "prettier-eslint-cli": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
